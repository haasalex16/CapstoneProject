 <% if (playlists.length === 0) { %>
   <p class="none">
     No Playlists Yet
   </p>
 <% } %>

<ul>
  <% playlists.each(function(playlist) { %>
    <li class='result-bold'>
    <button class="add-to-playlist" data-id="<%= playlist.id %>">Add to playlist</button>
    <%= playlist.escape('title') %>
    </li>
  <% }) %>
</ul>
