
<div class="album-art">
  Art
</div>

<div class="play-button">
  <div class="arrow"></div>
</div>

<a href="#/users/<%=song.get('owner').id %>"><%= song.get('owner').username %></a>

<% if(currentUser.get('id') === song.get('owner').id){ %>
  <a class='title' href="#/songs/<%= song.get('id') %>/edit">
    <%= song.escape('title') %>
  </a>
<%  } else { %>
  <p class='title'><%= song.escape('title') %> </p>
<%  }%>
<div class="music-player">
  <%= song.escape('created_at') %>
</div>
